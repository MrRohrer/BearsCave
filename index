<p class="p1"><span class="Apple-converted-space">            </span>showNotification('‚úèÔ∏è Feedback sent to author for improvements!');</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function rejectPost(button) {</p>
<p class="p1"><span class="Apple-converted-space">            </span>const postCard = button.closest('.border-2');</p>
<p class="p1"><span class="Apple-converted-space">            </span>postCard.style.background = 'linear-gradient(to right, #fecaca, #fca5a5)';</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification('‚ùå Post rejected. Author will be notified kindly!');</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
