<p class="p1"><span class="Apple-converted-space">        </span>function vote(option) {</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification(`üó≥Ô∏è Thanks for voting for ${option}! Your voice matters!`);</p>
<p class="p2"><span class="Apple-converted-space">            </span></p>
<p class="p1"><span class="Apple-converted-space">            </span>// Add visual feedback</p>
<p class="p1"><span class="Apple-converted-space">            </span>event.target.classList.add('pulse-glow');</p>
<p class="p1"><span class="Apple-converted-space">            </span>setTimeout(() =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">                </span>event.target.classList.remove('pulse-glow');</p>
<p class="p1"><span class="Apple-converted-space">            </span>}, 2000);</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function showSection(type) {</p>
<p class="p1"><span class="Apple-converted-space">            </span>const cards = document.querySelectorAll('.content-card');</p>
<p class="p2"><span class="Apple-converted-space">            </span></p>
<p class="p1"><span class="Apple-converted-space">            </span>if (type === 'all') {</p>
<p class="p1"><span class="Apple-converted-space">                </span>cards.forEach(card =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">                    </span>card.style.display = 'block';</p>
<p class="p1"><span class="Apple-converted-space">                    </span>card.classList.add('bounce-in');</p>
<p class="p1"><span class="Apple-converted-space">                </span>});</p>
<p class="p1"><span class="Apple-converted-space">                </span>showNotification('üì∞ Showing all content! So much to explore!');</p>
<p class="p1"><span class="Apple-converted-space">            </span>} else {</p>
<p class="p1"><span class="Apple-converted-space">                </span>cards.forEach(card =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">                    </span>if (card.dataset.type === type) {</p>
<p class="p1"><span class="Apple-converted-space">                        </span>card.style.display = 'block';</p>
<p class="p1"><span class="Apple-converted-space">                        </span>card.classList.add('bounce-in');</p>
<p class="p1"><span class="Apple-converted-space">                    </span>} else {</p>
<p class="p1"><span class="Apple-converted-space">                        </span>card.style.display = 'none';</p>
<p class="p1"><span class="Apple-converted-space">                    </span>}</p>
<p class="p1"><span class="Apple-converted-space">                </span>});</p>
<p class="p1"><span class="Apple-converted-space">                </span>showNotification(`üéØ Filtered to ${type}! Perfect choice!`);</p>
<p class="p1"><span class="Apple-converted-space">            </span>}</p>
<p class="p2"><span class="Apple-converted-space">            </span></p>
<p class="p1"><span class="Apple-converted-space">            </span>// Remove animation class after animation completes</p>
<p class="p1"><span class="Apple-converted-space">            </span>setTimeout(() =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">                </span>cards.forEach(card =&gt; card.classList.remove('bounce-in'));</p>
<p class="p1"><span class="Apple-converted-space">            </span>}, 600);</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>// Add some random fun interactions</p>
<p class="p1"><span class="Apple-converted-space">        </span>setInterval(() =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">            </span>const emojis = ['üåü', '‚≠ê', '‚ú®', 'üéâ', 'üöÄ', 'üí´'];</p>
<p class="p1"><span class="Apple-converted-space">            </span>const randomEmoji = emojis[Math.floor(Math.random() * emojis.length)];</p>
<p class="p2"><span class="Apple-converted-space">            </span></p>
<p class="p1"><span class="Apple-converted-space">            </span>// Occasionally show encouraging messages</p>
<p class="p1"><span class="Apple-converted-space">            </span>if (Math.random() &lt; 0.1) {</p>
<p class="p1"><span class="Apple-converted-space">                </span>showNotification(`You're doing great! ${randomEmoji}`);</p>
<p class="p1"><span class="Apple-converted-space">            </span>}</p>
<p class="p1"><span class="Apple-converted-space">        </span>}, 30000);</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>// Create Post Modal Functions</p>
<p class="p1"><span class="Apple-converted-space">        </span>function openCreateModal() {</p>
<p class="p1"><span class="Apple-converted-space">            </span>document.getElementById('createModal').classList.remove('hidden');</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification('‚úèÔ∏è Time to create something awesome!');</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function closeCreateModal() {</p>
<p class="p1"><span class="Apple-converted-space">            </span>document.getElementById('createModal').classList.add('hidden');</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function selectPostType(type, button) {</p>
<p class="p1"><span class="Apple-converted-space">            </span>// Remove selection from all buttons</p>
<p class="p1"><span class="Apple-converted-space">            </span>document.querySelectorAll('.post-type-btn').forEach(btn =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">                </span>btn.classList.remove('ring-4', 'ring-blue-400');</p>
<p class="p1"><span class="Apple-converted-space">            </span>});</p>
<p class="p2"><span class="Apple-converted-space">            </span></p>
<p class="p1"><span class="Apple-converted-space">            </span>// Add selection to clicked button</p>
<p class="p1"><span class="Apple-converted-space">            </span>button.classList.add('ring-4', 'ring-blue-400');</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification(`üìù ${type} selected! Great choice!`);</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function toggleTag(button) {</p>
<p class="p1"><span class="Apple-converted-space">            </span>if (button.classList.contains('bg-blue-500')) {</p>
<p class="p1"><span class="Apple-converted-space">                </span>button.classList.remove('bg-blue-500', 'text-white');</p>
<p class="p1"><span class="Apple-converted-space">                </span>button.classList.add('bg-gray-200');</p>
<p class="p1"><span class="Apple-converted-space">            </span>} else {</p>
<p class="p1"><span class="Apple-converted-space">                </span>button.classList.remove('bg-gray-200');</p>
<p class="p1"><span class="Apple-converted-space">                </span>button.classList.add('bg-blue-500', 'text-white');</p>
<p class="p1"><span class="Apple-converted-space">            </span>}</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function submitPost() {</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification('üöÄ Post submitted for review! You\'re making a difference!');</p>
<p class="p1"><span class="Apple-converted-space">            </span>closeCreateModal();</p>
<p class="p2"><span class="Apple-converted-space">            </span></p>
<p class="p1"><span class="Apple-converted-space">            </span>// Update dashboard stats</p>
<p class="p1"><span class="Apple-converted-space">            </span>const postsCount = document.querySelector('.text-yellow-300');</p>
<p class="p1"><span class="Apple-converted-space">            </span>if (postsCount) {</p>
<p class="p1"><span class="Apple-converted-space">                </span>postsCount.textContent = parseInt(postsCount.textContent) + 1;</p>
<p class="p1"><span class="Apple-converted-space">            </span>}</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function saveDraft() {</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification('üíæ Draft saved! You can finish it later!');</p>
<p class="p1"><span class="Apple-converted-space">            </span>closeCreateModal();</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>// Moderation Modal Functions</p>
<p class="p1"><span class="Apple-converted-space">        </span>function openModerateModal() {</p>
<p class="p1"><span class="Apple-converted-space">            </span>document.getElementById('moderateModal').classList.remove('hidden');</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification('üõ°Ô∏è Welcome to the moderation center!');</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function closeModerationModal() {</p>
