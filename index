<p class="p1"><span class="Apple-converted-space">        </span>function approvePost(button) {</p>
<p class="p1"><span class="Apple-converted-space">            </span>const postCard = button.closest('.border-2');</p>
<p class="p1"><span class="Apple-converted-space">            </span>postCard.style.background = 'linear-gradient(to right, #dcfce7, #bbf7d0)';</p>
<p class="p1"><span class="Apple-converted-space">            </span>postCard.innerHTML = '&lt;div class="text-center py-8"&gt;&lt;div class="text-4xl mb-2"&gt;‚úÖ&lt;/div&gt;&lt;h3 class="text-xl font-bold text-green-600"&gt;Post Approved!&lt;/h3&gt;&lt;p class="text-green-700"&gt;This story is now live on Bears Den!&lt;/p&gt;&lt;/div&gt;';</p>
<p class="p1"><span class="Apple-converted-space">            </span>showNotification('‚úÖ Post approved! Great job keeping quality high!');</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">        </span>function requestChanges(button) {</p>
<p class="p1"><span class="Apple-converted-space">            </span>const postCard = button.closest('.border-2');</p>
